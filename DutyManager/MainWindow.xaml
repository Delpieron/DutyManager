<Window x:Class="DutyManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DutyManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="GroupsLabel" Content="Twoje Grupy" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="207"/>
        <Button x:Name="GroupAddButton" Content="Utwórz Grupę" HorizontalAlignment="Left" Margin="340,125,0,0" VerticalAlignment="Top" Click="GroupAddButton_Click"/>
        <Button x:Name="DeleteGroupButton" Content="Usuń Grupę" HorizontalAlignment="Left" Margin="340,150,0,0" VerticalAlignment="Top" Width="77"/>
        <ListBox x:Name="ListGroup" Margin="0,69,686,0"/>
    </Grid>
</Window>
